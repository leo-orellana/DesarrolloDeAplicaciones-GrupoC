<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false" default-access="field"
	default-cascade="save-update">
	<class name="ar.edu.unq.desapp.grupoc.model.Supplier" table="suppliers">
		<id name="id" type="int" column="id">
	        <generator class="native"/>
	    </id>
		<property name="companyName" type="string"></property>
		<property name="code" type="string"></property>
		<property name="cuit" type="string"></property>
	</class>
</hibernate-mapping>